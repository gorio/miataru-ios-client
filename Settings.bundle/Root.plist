<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>PreferenceSpecifiers</key>
	<array>
		<dict>
			<key>Title</key>
			<string>Client Settings</string>
			<key>Type</key>
			<string>PSGroupSpecifier</string>
			<key>FooterText</key>
			<string>Enable or Disable Tracking and Server Reporting</string>
		</dict>
		<dict>
			<key>DefaultValue</key>
			<true/>
			<key>Key</key>
			<string>track_and_report_location</string>
			<key>Title</key>
			<string>Track and Report Location</string>
			<key>Type</key>
			<string>PSToggleSwitchSpecifier</string>
		</dict>
		<dict>
			<key>Title</key>
			<string>Location History</string>
			<key>Type</key>
			<string>PSGroupSpecifier</string>
			<key>FooterText</key>
			<string>Enable Location History if you want to store a Location History on the Miataru Server (Number is limited by the Server).</string>
		</dict>
		<dict>
			<key>DefaultValue</key>
			<false/>
			<key>Key</key>
			<string>save_location_history_on_server</string>
			<key>Title</key>
			<string>Server Location History</string>
			<key>Type</key>
			<string>PSToggleSwitchSpecifier</string>
		</dict>
		<dict>
			<key>Title</key>
			<string>Server Settings</string>
			<key>Type</key>
			<string>PSGroupSpecifier</string>
			<key>FooterText</key>
			<string>Here you need to specify the server you want to use. By default service.miataru.com is set here but you can use your own.</string>
		</dict>
		<dict>
			<key>Type</key>
			<string>PSTextFieldSpecifier</string>
			<key>DefaultValue</key>
			<string>http://service.miataru.com</string>
			<key>Title</key>
			<string>Server-URL</string>
			<key>Key</key>
			<string>miataru_server_url</string>
		</dict>
		<dict>
			<key>Type</key>
			<string>PSGroupSpecifier</string>
			<key>Title</key>
			<string>Map Update Interval</string>
			<key>FooterText</key>
			<string>How often do you want to get the location from other devices in your Known Devices list. This is not the update interval for reporting locations.</string>
		</dict>
		<dict>
			<key>DefaultValue</key>
			<string>30</string>
			<key>Key</key>
			<string>map_update_interval</string>
			<key>Title</key>
			<string>Update Interval</string>
			<key>Titles</key>
			<array>
				<string>5s</string>
				<string>10s</string>
				<string>15s</string>
				<string>30s</string>
				<string>60s</string>
			</array>
			<key>Type</key>
			<string>PSMultiValueSpecifier</string>
			<key>Values</key>
			<array>
				<string>5</string>
				<string>10</string>
				<string>15</string>
				<string>30</string>
				<string>60</string>
			</array>
		</dict>
	</array>
	<key>StringsTable</key>
	<string>Root</string>
</dict>
</plist>
